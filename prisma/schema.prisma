generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Booking {
  id Int @id @default(autoincrement())
  username String
  email String 
  phoneNumber String
  price Int
  distance Float
  route String
  fromLocation Json
  toLocation Json
  createdAt String
  pickupdDate String
  pickupdTime String
  dropDate String
  dropTime String
  details Json
}