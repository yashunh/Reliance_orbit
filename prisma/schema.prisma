generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Booking {
  id Int @id @default(autoincrement())
  username String
  email String 
  phoneNumber String
  price Int
  distance Float
  route String
  duration String
  pickupdDate String
  pickupdTime String
  pickupAddress Json
  dropDate String
  dropTime String
  dropAddress Json
  vanType String
  worker Int 
  fromLocation Json
  toLocation Json
  details Json
  quotationRef String
  createdAt DateTime @default(now())
}